<div class="container">
  <strong>
    <div class='row'>
      <div class='col-md-1'>Order Id </div>
      <div class='col-md-2'>billing_address </div>
      <div class='col-md-2'>shipping_address </div>
      <div class='col-md-1'>status</div>
      <div class='col-md-1'>User ID</div>
      <div class='col-md-1'>Subtotal</div>
      <div class='col-md-1'>Tax</div>
      <div class='col-md-1'>Total</div>
    </div>
  </strong>
  <hr>
  <% @orders.each do |order| %>
    <div class='row'>
      <div class='col-md-1'> <%= order.id %> </div>
      <div class='col-md-2'> <%= order.billing_address %> </div>
      <div class='col-md-2'> <%= order.shipping_address %> </div>
      <div class='col-md-1'> <%= order.status %> </div>
      <div class='col-md-1'> <%= order.user_id %> </div>
      <div class='col-md-1'> <%= order.subtotal %> </div>
      <div class='col-md-1'> <%= order.tax %> </div>
      <div class='col-md-1'> <%= order.total %> </div>
      <div class='col-md-2'> <%= link_to 'Show', order, class: "btn btn-xs btn-primary" %> </div>
    </div>
  <% end %>
</div>